<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="576.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.scene.MainScreenController">
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label text="HBSS">
               <font>
                  <Font name="VL-Code-Pro-Bold" size="100.0" />
               </font>
            </Label>
            <Label text="Heap Bubble Shell Sort">
               <font>
                  <Font name="VL-Code-Pro-Bold" size="19.0" />
               </font>
            </Label>
         </children></VBox>
   </center>
   <right>
      <VBox alignment="CENTER" prefHeight="576.0" prefWidth="300.0" spacing="10.0" style="-fx-background-color: #c7c7c7;" BorderPane.alignment="CENTER">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#switchToSortScene" text="Start Sorting">
               <font>
                  <Font size="30.0" />
               </font></Button>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#quit" text="Quit">
               <font>
                  <Font size="30.0" />
